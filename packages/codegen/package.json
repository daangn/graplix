{
  "name": "@graplix/codegen",
  "version": "2.1.3",
  "description": "Code generation toolkit and CLI for Graplix TypeScript helpers",
  "keywords": [
    "graplix",
    "codegen",
    "cli",
    "typescript",
    "rebac",
    "mappers",
    "schema"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/daangn/graplix.git",
    "directory": "packages/codegen"
  },
  "license": "MIT",
  "author": "Danggeun Market Inc.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./schema.json": "./schema.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "graplix-codegen": "./dist/cli.mjs"
  },
  "files": [
    "dist",
    "schema.json"
  ],
  "scripts": {
    "build": "tsdown",
    "codegen": "node ./dist/cli.mjs",
    "test": "vitest --run --passWithNoTests"
  },
  "dependencies": {
    "@graplix/language": "^2.0.0",
    "cosmiconfig": "^9.0.0",
    "cosmiconfig-typescript-loader": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^24.10.13",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
